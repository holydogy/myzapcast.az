{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/admin",
            "destination": "/admin.html"
        },
        {
            "source": "/premium",
            "destination": "/premium-landing.html"
        },
        {
            "source": "/hero",
            "destination": "/hero-landing.html"
        }
    ]
}